   "source": [
    "import os\n",
    "\n",
    "api_token = os.getenv('FMP_API_TOKEN')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "functional-processing",
   "metadata": {},
   "outputs": [],
   "source": [
    "import requests\n",
    "\n",
    "url = f'https://financialmodelingprep.com/api/v3/stock/list?apikey={api_token}'\n",
    "\n",
    "response = requests.get(url).json()\n",
    "\n",
    "symbols = []\n",
    "\n",
    "for symbol in response:\n",
    "    symbols.append(symbol['symbol'])\n",
    "\n",
    "symbols"
   ]
  }
 ],
